<html>
    <head>
        <title>
            Авторизація
        </title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <script src="script/jquery-3.2.1.js" type="text/javascript"></script>
        <style>
            body{
                margin: 0;
                width: 100%;
                height: 100%;
                position: relative;
                background: url("data:image/png;base64,");
                background-color: #6c6e71;
                background-repeat: no-repeat;
                background-position: center 37%;
            }

            .auth{
                width: 460px;
                /* background: rgba(0, 0, 0, 0.55); */
                position: absolute;
                left: 50%;
                top: 50%;
                transform: translate(-50%,-50%);
                /* border: 1px solid #000000; */
                border-radius: 0px;
                /* box-shadow: -2px 2px 8px 1px black; */
                /* background: #383e45 url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAGQAAABkCAYAAABw4pVUAAAfqElEQ…Ck43x0Vq2AYvMpkrVjk2rofvYhnTnE3aIAFLNT97b3/gWArweacYg5JgAAAABJRU5ErkJggg==); */
                border-radius: 5px;
            }

            .auth input[type=text],.auth input[type=password]{
                width: 98%;
                height: 35px;
                margin: 20px 6px 0px 4px;
                background: transparent;
                border: 0;
                border-bottom: 2px solid white;
                color: white;
                text-align: center;
                font-size: 22px;
                font-family: sans-serif;
            }

            .auth input[type=submit]{
                width: 98%;
                height: 35px;
                /* margin-top: 20px; */
                margin: 20px 6px 0px 4px;
                border: 3px solid white;
                background: white;
                font-size: 18px;
                font-family: sans-serif;
                color: black;
                cursor: pointer;
                transition: 0.4s;
            }

            .auth input[type=submit]:hover{
                background: black;
                color: white
            }

            .a2{
                display: none
            }
            .sms-txt{
                width: 100%;
                height: 15px;
                color: white;
                text-align: center;
                font-family: sans-serif;
                line-height: 27px;
            }
        </style>
    </head>
    <body>
        <div class="auth block">
            <!--<form action="/panel" method="GET">-->
            <input class="a1" type="text" name="login" placeholder="Логін" id="log">
            <input class="a1" type="password" name="pass" placeholder="Пароль" id="pass">
            <input class="a1" type="submit" value="Вхід" id="authBtn" name="authBtn">
            <div class="a2 sms-txt">Вам на Email відправлено код, ввудіть його тут</div>
            <input class="a2" type="text" name="sms" placeholder="Логін" id="sms">
            <input class="a2" type="submit" value="Підтвердити СМС" id="SendSMS" name="sendsms">
            <!--</form>-->
            <script>
                $('#authBtn').click(function () {
                    $.post('/auth', {L: $('#log').val(), P: $('#pass').val()}, function (data) {
                        if (parseInt(JSON.parse(data).code) === 500) {
                            $('.a1').fadeOut(300);
                            $('.a2').fadeIn(300);
                        }
                    })
                });

                $('#SendSMS').click(function () {
                    $.post('/authSMS', {S: $('#sms').val(), N: $('#log').val()}, function (data) {
                        if (parseInt(JSON.parse(data).code) === 500) {
                            document.location.href = JSON.parse(data).url;
                        }
                    });
                });

//                document.location.href = JSON.parse(data).loc
            </script>
        </div>
    </body>
</html>