<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Різник</title>
        <link href="style/index-style.css" rel="stylesheet" type="text/css"/>
        <link href="style/min.index-style.css" rel="stylesheet" type="text/css"/>
        <link href="style/swiper.min.css" rel="stylesheet" type="text/css"/>
        
        <script type="text/javascript" src="script/jquery-3.2.1.js"></script>
        <script src="script/swiper.min.js" type="text/javascript"></script>      
    </head>
    <body>
        <div id="document-preload">
            <div id="load">
                <div>G</div>
                <div>N</div>
                <div>I</div>
                <div>D</div>
                <div>A</div>
                <div>O</div>
                <div>L</div>
            </div>
        </div>
        <div class="content blur">
            <div class="block" id="head"> 
                <div class="swiper-container">
                  <div class="swiper-wrapper">
                      <% for(var i = 0; i < slides.length; i++) { %>
                        <div class="swiper-slide">
                          <div class="slide-inner" style="background-image: url(../image/slide/<%= slides[i] %>)"></div>
                        </div>
                      <% } %>                   
                  </div>
                  <div class="swiper-button-next swiper-button-white"></div>
                  <div class="swiper-button-prev swiper-button-white"></div>
                </div>
                <div class="head-menu">
                    <div class="LOGOTYPE"></div>
                    <div class="center">
                        <div class="slash">~</div>
                        <div class="head-btn md-trigger" data-modal="modal-10">Онлайн магазин</div>
                        <div class="slash">~</div>
                        <div class="head-btn">Акції</div>
                        <div class="slash">~</div>
                        <div class="head-btn">Про нас</div>
                        <div class="slash">~</div>
                    </div>
                </div>
                
                <div class="footer">
                    <div class="foot-L"></div>
                    <div class="foot-C">
                        
                    </div>
                    <div class="foot-R">
                        <div class="socials" id="instagramm"></div>
                        <div class="socials" id="facebook"></div>
                        <div class="socials" id="twitter"></div>
                        <div class="socials" id="whatsapp"></div>
                    </div>
                </div>
            </div>  
            <div class="block" id="about">
                <div class="about-head"></div>
            </div>
            <div class="md-modal md-effect-10" id="modal-10">
                    <div class="md-content">
                        <iframe src="shop"></iframe>
                    </div>
            </div>
        </div>
        
        <script type="text/javascript" src="script/index-script.js"></script>
    </body>        
</html>
